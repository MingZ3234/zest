<?xml version="1.0" encoding="UTF-8"?>

<configuration status="OFF">
    <appenders>

        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%msg%n"/>
        </Console>
    </appenders>

    <loggers>
        <Root level="info">
            <appender-ref ref="Console"/>
        </Root>
    </loggers>
</configuration>